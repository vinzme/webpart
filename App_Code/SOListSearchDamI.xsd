                  <DbCommand CommandType="Text" ModifiedByUser="True">
                    <CommandText>Select substring(SES.scheme.projects.project_code,1,6) as project_code, substring(SES.scheme.projects.cost_centre,4,3) as cc,
substring(SES.scheme.projects.cost_centre,1,2) as div, SES.scheme.projects.project_status as status,
SES.scheme.projects.customer as customer, SES.scheme.projects.custname,
SES.scheme.projects.project_type, SES.scheme.projects.sitename, rtrim(convert(char,datepart(dd,SES.scheme.projects.start_date)))+'-'+rtrim(convert(char,datepart(mm,SES.scheme.projects.start_date)))+'-'+rtrim(convert(char,datepart(yyyy,SES.scheme.projects.start_date))) as start_date, rtrim(convert(char,datepart(dd,SES.scheme.projects.end_date)))+'-'+rtrim(convert(char,datepart(mm,SES.scheme.projects.end_date)))+'-'+rtrim(convert(char,datepart(yyyy,SES.scheme.projects.end_date))) as end_date
from SES.scheme.projects INNER JOIN and_users_cc ON SES.scheme.projects.cost_centre = and_users_cc.costcenter
where SES.scheme.projects.project_code+SES.scheme.projects.description+SES.scheme.projects.customer+SES.scheme.projects.analysis01+ 
SES.scheme.projects.analysis04+SES.scheme.projects.custname+SES.scheme.projects.sitename like @SearchKey and substring(ltrim(SES.scheme.projects.project_code),8,1) = 'I'
and and_users_cc.userid = @SoUser
 UNION 
Select substring(SES.scheme.projects.project_code,1,6) as project_code, substring(SES.scheme.projects.cost_centre,4,3) as cc,
substring(SES.scheme.projects.cost_centre,1,2) as div, SES.scheme.projects.project_status as status,
SES.scheme.projects.customer as customer, SES.scheme.projects.custname,
SES.scheme.projects.project_type, SES.scheme.projects.sitename, rtrim(convert(char,datepart(dd,SES.scheme.projects.start_date)))+'-'+rtrim(convert(char,datepart(mm,SES.scheme.projects.start_date)))+'-'+rtrim(convert(char,datepart(yyyy,SES.scheme.projects.start_date))) as start_date, rtrim(convert(char,datepart(dd,SES.scheme.projects.end_date)))+'-'+rtrim(convert(char,datepart(mm,SES.scheme.projects.end_date)))+'-'+rtrim(convert(char,datepart(yyyy,SES.scheme.projects.end_date))) as end_date
from SES.scheme.projects INNER JOIN and_users_cc ON SES.scheme.projects.cost_centre = and_users_cc.costcenter
where SES.scheme.projects.project_code+SES.scheme.projects.description+SES.scheme.projects.customer+SES.scheme.projects.analysis01+ 
SES.scheme.projects.analysis04+SES.scheme.projects.custname+SES.scheme.projects.sitename like @SearchKey and substring(ltrim(SES.scheme.projects.project_code),8,1) &lt;&gt; 'I' and substring(cost_centre,1,2) = '28'
and and_users_cc.userid = @SoUser order by substring(SES.scheme.projects.project_code,1,6) desc</CommandText>
